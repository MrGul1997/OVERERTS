*{
	list-style:none;
	margin:0;
	text-decoration:none;
}

.container{
	width:100%;
	
}

.nav{
	
	background-color: #333;
	
	
}

.left-side{
	position:relative;
	width:30%;
	
}

.left-side h2{
	position:absolute;
	color:white;
	text-transform:uppercase;
	top:20px;
	left:30%;
}

.right-side{
	position:relative;
	width:100%;
	height:10vh;
}

.right-side ul{
	top:20px;
	position:absolute;
	margin-left:38%;
}

.right-side ul li{
	cursor:pointer;
	font-size:20px;
	color:white;
	float:left;
	margin-left:30px;
	display: block;	
	
}

.right-side ul li:hover{
	color:black;
	transition-duration:2s;
	transform:scale(1.5);
	background: #ddd;
	padding: 10px 16px;

}

.clear{
	clear:both;
}

.header{
	position:relative;
	width:100%;
	height:90vh;
	background-image:linear-gradient(rgba(0,0,0,.7),rgba(0,0,0,.7)),url(header.jpg);
	background-size:100% 100%;
	background-attachment:fixed;
}

.header h2{
	position:absolute;
	color:white;
	font-weight:normal;
	left:20%;
	top:20%;
	text-transform:uppercase;
}

.header h1{
	position:absolute;
	color:white;
	left:20%;
	top:30%;
	text-transform:uppercase;
}

.header p{
	position:absolute;
	color:white;
	font-weight:normal;
	left:20%;
	top:45%;
	text-transform:uppercase;
}

.btn1{
	position:absolute;
	background:tomato;
	border:none;
	padding:10px 20px;
	color:white;
	left:20%;
	top:55%;
	text-transform:uppercase;
	cursor:pointer;
}

.btn1:hover{
	transform:scale(1.2);
	transition-duration:1.5s;
}
.btn2{
	position:absolute;
	background:transparent;
	border:none;
	padding:10px 15px;
	
	border:1px solid white;
	color:white;
	left:30%;
	top:55%;
	text-transform:uppercase;
	cursor:pointer;
}

.btn2:hover{
	background:white;
	color:black;
	transition-duration:1.5s;
  
}

